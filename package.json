    "file-saver": "2.0.2",
    "fuse.js": "3.6.1",
    "js-cookie": "2.2.1",
    "jsonlint": "1.6.3",
    "jszip": "3.7.0",
    "lodash": "^4.17.17",
    "normalize.css": "7.0.0",
    "nprogress": "0.2.0",
    "opentmi-addon": "^0.2.0",
    "path-to-regexp": "2.4.0",
    "schema-json-vue": "^1.0.18",
    "screenfull": "4.2.1",
    "script-loader": "0.7.2",
    "showdown": "1.9.1",
    "socket.io-client": "^2.3.0",
    "sortablejs": "1.10.2",
